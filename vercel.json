{
  "version": 2,
  "builds": [
    {
      "src": "vortexiwebsite/app/__init__.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vortexiwebsite/app/__init__.py"
    }
  ],
  "functions": {
    "vortexiwebsite/app/__init__.py": {
      "runtime": "python3.9",
      "memory": 3008,
      "maxDuration": 30
    }
  },
  "build": {
    "env": {
      "PYTHON_VERSION": "3.9"
    }
  },
  "installCommand": "bash vortexiwebsite/start.sh || true",
  "buildCommand": "echo 'Build complete'"
}
