{
  "builds": [
    {
      "src": "vortexiwebsite/app/app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/app",
      "dest": "vortexiwebsite/app/app.py"
    },
    {
      "src": "/(.*)",
      "dest": "vortexiwebsite/app/app.py"
    }
  ],
  "functions": {
    "vortexiwebsite/app/app.py": {
      "runtime": "python3.9",
      "maxDuration": 10
    }
  },
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}
